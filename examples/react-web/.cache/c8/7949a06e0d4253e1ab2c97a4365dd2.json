ne":81,"column":34},"source":"../../../src/plugins/events/events.js","original":{"line":71,"column":34}},{"generated":{"line":81,"column":39},"source":"../../../src/plugins/events/events.js","original":{"line":71,"column":11}},{"name":"action","generated":{"line":81,"column":41},"source":"../../../src/plugins/events/events.js","original":{"line":71,"column":41}},{"generated":{"line":81,"column":47},"source":"../../../src/plugins/events/events.js","original":{"line":71,"column":11}},{"generated":{"line":82,"column":0},"source":"../../../src/plugins/events/events.js","original":{"line":69,"column":14}},{"generated":{"line":82,"column":7},"source":"../../../src/plugins/events/events.js","original":{"line":69,"column":6}},{"generated":{"line":83,"column":0},"source":"../../../src/plugins/events/events.js","original":{"line":73,"column":5}},{"generated":{"line":85,"column":0},"source":"../../../src/plugins/events/events.js","original":{"line":74,"column":4}},{"name":"state","generated":{"line":85,"column":11},"source":"../../../src/plugins/events/events.js","original":{"line":74,"column":11}},{"generated":{"line":85,"column":16},"source":"../../../src/plugins/events/events.js","original":{"line":74,"column":4}},{"generated":{"line":86,"column":0},"source":"../../../src/plugins/events/events.js","original":{"line":75,"column":3}},{"generated":{"line":88,"column":0},"source":"../../../src/plugins/events/events.js","original":{"line":14,"column":20}}],"sources":{"../../../src/plugins/events/events.js":"/*\n * Copyright 2018 The boardgame.io Authors\n *\n * Use of this source code is governed by a MIT-style\n * license that can be found in the LICENSE file or at\n * https://opensource.org/licenses/MIT.\n */\n\nimport { automaticGameEvent } from '../../core/action-creators';\n\n/**\n * Events\n */\nexport class Events {\n  constructor(flow, playerID) {\n    this.flow = flow;\n    this.playerID = playerID;\n    this.dispatch = [];\n  }\n\n  /**\n   * Attaches the Events API to ctx.\n   * @param {object} ctx - The ctx object to attach to.\n   */\n  api(ctx) {\n    const events = {};\n    const { phase, turn } = ctx;\n\n    for (const key of this.flow.eventNames) {\n      events[key] = (...args) => {\n        this.dispatch.push({ key, args, phase, turn });\n      };\n    }\n\n    events._obj = this;\n\n    return events;\n  }\n\n  isUsed() {\n    return this.dispatch.length > 0;\n  }\n\n  /**\n   * Updates ctx with the triggered events.\n   * @param {object} state - The state object { G, ctx }.\n   */\n  update(state) {\n    for (let i = 0; i < this.dispatch.length; i++) {\n      const item = this.dispatch[i];\n\n      // If the turn already ended some other way,\n      // don't try to end the turn again.\n      if (item.key === 'endTurn' && item.turn !== state.ctx.turn) {\n        continue;\n      }\n\n      // If the phase already ended some other way,\n      // don't try to end the phase again.\n      if (\n        (item.key === 'endPhase' || item.key === 'setPhase') &&\n        item.phase !== state.ctx.phase\n      ) {\n        continue;\n      }\n\n      const action = automaticGameEvent(item.key, item.args, this.playerID);\n\n      state = {\n        ...state,\n        ...this.flow.processEvent(state, action),\n      };\n    }\n    return state;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"92a7294a8c4eb41063cfa91a05bc42ab","cacheData":{"env":{}}}